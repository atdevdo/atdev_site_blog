{% macro macro_atdev(atdev_id, atdev_title, atdev_link, atdev_img, atdev_img_alt) %}
	<div class="atdev">
		<div id="{{ atdev_id }}" class="atdev-content">
			<h2>{{ atdev_title }}</h2>
			<a onclick="mixpanel.track('Go to ' + atdev_title + ' Page Button')" href="{{ atdev_link }}">Check Our Web Page</a>
		</div>
		<div class="atdev-imgBox"  style="background-image: url({{atdev_img}});"></div>
	</div>
{% endmacro %}