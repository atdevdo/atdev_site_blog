{% block macro_custom_footer %}
  <style>
    .custom-footer {
      z-index: 1 !important;
      margin-top: 4em !important;
    }

    .custom-footer-item {
      margin-top: 0em !important;
      margin-bottom: 62px !important;
    }

    @media only screen and (min-width: 1000px) {
      .custom-footer-item {
        margin-top: -3em !important;
        margin-bottom: 62px !important;
      }
    }

    .custom-footer-first-bg {
      position: relative !important;
      height: 21em !important;
    }

    .custom-footer-first-bg .main-banner-content {
      margin-top: 4em !important;
    }

    .custom-footer-second-bg {
      position: relative !important;
    }

    .custom-footer-first-bg,
    .custom-footer-second-bg,
    .footer-bottom {
      width: 100% !important;
      color: #fff !important;
      /* background-color: #303036; */
    }

    .custom-footer .footer-main {
      padding-bottom: 0 !important;
    }

    .custom-footer-bk-banner {
      position: absolute !important;
      width: 100% !important;
      height: 100% !important;
      top: 0 !important;
      z-index: -1 !important;
      object-fit: cover !important;
    }

    @media only screen and (min-width: 1000px) {
      .custom-footer-first-bg .custom-footer-bk-banner {
        height: 200% !important;
      }
    }

    .custom-footer .footer-company-desc {}

    .custom-footer .footer-company-desc p {
      color: #CACDD1 !important;
    }

    .custom-footer .copyright {
      transform: translateY(1.3em);
    }

    .custom-footer .footer-bottom .items {
      margin-bottom: 0 !important;
    }

    .custom-footer .footer-bottom {
      padding: 0 !important;
    }

    .custom-footer-icon {
      /* fill: #fff !important; */
    }
  </style>

  <footer class="custom-footer footer">
    {% if customFooterBanner %}
      <div class="custom-footer-first-bg">
        <img class="custom-footer-bk-banner" src="{{customFooterBanner}}" alt="">
        <div class="container">
          <div class="row items">
            <div class="col-xl-3 col-lg-3 col-md-5 col-12 item">
              <div class="main-banner-content">
                <h2 class="main-banner-title">{{data.find.title}}</h2>
                <div class="main-banner-desc">
                  <h2 class="main-banner-title">{{data.find.desc}}</h2>
                  <div class="row d-flex justify-content-left" style="margin-left: 0; margin-top: 1rem">
                    <a style="font-weight: bold !important;" onclick="mixpanel.track('Go to Contact Page Button')" href="{{" /" + locale + "/services/forms/" }}" class="btn btn-w240 ripple">{{data.find.btn}}</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    {% endif %}
    <div class="custom-footer-second-bg">
      {# <img class="custom-footer-bk-banner" src="{{customFooterBanner1}}" alt=""> #}
      <img class="custom-footer-bk-banner" src="/assets/images/footer_background.jpg" alt="">
      <div class="footer-main">
        <div class="container">
          <div class="row items" style="justify-content: space-around;">
            <div class="col-xl-3 col-lg-3 col-md-5 col-12 item custom-footer-item">
              <!-- Begin company info -->
              <div class="footer-company-info">
                <div class="footer-company-top">
                  <a onclick="mixpanel.track('Go Home Button')" href="/{{locale}}/" class="logo logo-ico-widht-text" title="ATDEV">
                    <img class="lazy logo-ico" data-src="/assets/images/logo.png" height="35" src="data:image/gif;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" alt="">
                    <span class="logo-text"></span>
                  </a>
                  <div class="footer-company-desc">
                    <p>{{ footer[locale].companyDescription }}</p>
                  </div>
                </div>
                <ul class="footer-social-links">
                  <li>
                    <a onclick="mixpanel.track('Go to LinkedIn Page Button')" href="https://www.linkedin.com/company/atdev/about/" title="LinkedIn">
                    {# <img class="custom-footer-icon" src="/icons/linkedin.svg" alt=""> #}
                    <svg class="custom-footer-icon" id="Bold" enable-background="new 0 0 24 24" height="512" viewBox="0 0 24 24" width="512" xmlns="http://www.w3.org/2000/svg"><path d="m23.994 24v-.001h.006v-8.802c0-4.306-.927-7.623-5.961-7.623-2.42 0-4.044 1.328-4.707 2.587h-.07v-2.185h-4.773v16.023h4.97v-7.934c0-2.089.396-4.109 2.983-4.109 2.549 0 2.587 2.384 2.587 4.243v7.801z"/><path d="m.396 7.977h4.976v16.023h-4.976z"/><path d="m2.882 0c-1.591 0-2.882 1.291-2.882 2.882s1.291 2.909 2.882 2.909 2.882-1.318 2.882-2.909c-.001-1.591-1.292-2.882-2.882-2.882z"/></svg>
                      {# <svg viewBox="0 0 448 512">
                        <use xlink:href="/assets/images/sprite.svg#linkedin-in-ico"></use>
                      </svg> #}
                    </a>
                  </li>
                  <li>
                    <a onclick="mixpanel.track('Go to Twitter Page Button')" href="https://twitter.com/AtdevConsulting" title="Twitter">
                      {# <img class="custom-footer-icon" src="/icons/social-media.svg" alt=""> #}
                      <svg class="custom-footer-icon" id="Layer_1" enable-background="new 0 0 512 512" height="512" viewBox="0 0 512 512" width="512" xmlns="http://www.w3.org/2000/svg"><path d="m392.599 13.5h-273.198c-58.394 0-105.901 47.507-105.901 105.901v273.198c0 58.394 47.507 105.901 105.901 105.901h273.198c58.394 0 105.901-47.507 105.901-105.901v-273.198c0-58.394-47.507-105.901-105.901-105.901zm80.901 379.099c0 44.609-36.292 80.901-80.901 80.901h-273.198c-44.609 0-80.901-36.292-80.901-80.901v-273.198c0-44.609 36.292-80.901 80.901-80.901h273.198c44.609 0 80.901 36.292 80.901 80.901zm-183.074-159.532 94.35-117.595c4.32-5.385 3.457-13.252-1.927-17.573-5.385-4.319-13.252-3.458-17.573 1.928l-90.876 113.265-90.876-113.265c-2.373-2.957-5.959-4.678-9.75-4.678h-73.601c-4.808 0-9.19 2.758-11.27 7.093s-1.489 9.479 1.52 13.229l131.15 163.462-94.35 117.595c-4.32 5.385-3.457 13.252 1.927 17.573 2.308 1.851 5.07 2.75 7.815 2.75 3.66 0 7.289-1.601 9.758-4.678l90.877-113.265 90.875 113.265c2.373 2.957 5.959 4.678 9.75 4.678h73.601.02c6.903 0 12.5-5.596 12.5-12.5 0-3.187-1.192-6.094-3.155-8.302zm53.797 158.784-217.994-271.702h41.548l217.994 271.702z"/></svg>
                      {# <svg viewBox="0 0 512 512">
                        <use xlink:href="/assets/images/sprite.svg#twitter-ico"></use>
                      </svg> #}
                    </a>
                  </li>
                  <li>
                    <a onclick="mixpanel.track('Go to Instagram Page Button')" href="https://instagram.com/atdev.do?igshid=NmZiMzY2Mjc=" title="Twitter">
                    {# <img class="custom-footer-icon" src="/icons/instagram.svg" alt=""> #}
                    <svg class="custom-footer-icon" viewBox="0 0 512.00096 512.00096" xmlns="http://www.w3.org/2000/svg"><path d="m373.40625 0h-234.8125c-76.421875 0-138.59375 62.171875-138.59375 138.59375v234.816406c0 76.417969 62.171875 138.589844 138.59375 138.589844h234.816406c76.417969 0 138.589844-62.171875 138.589844-138.589844v-234.816406c0-76.421875-62.171875-138.59375-138.59375-138.59375zm108.578125 373.410156c0 59.867188-48.707031 108.574219-108.578125 108.574219h-234.8125c-59.871094 0-108.578125-48.707031-108.578125-108.574219v-234.816406c0-59.871094 48.707031-108.578125 108.578125-108.578125h234.816406c59.867188 0 108.574219 48.707031 108.574219 108.578125zm0 0"/><path d="m256 116.003906c-77.195312 0-139.996094 62.800782-139.996094 139.996094s62.800782 139.996094 139.996094 139.996094 139.996094-62.800782 139.996094-139.996094-62.800782-139.996094-139.996094-139.996094zm0 249.976563c-60.640625 0-109.980469-49.335938-109.980469-109.980469 0-60.640625 49.339844-109.980469 109.980469-109.980469 60.644531 0 109.980469 49.339844 109.980469 109.980469 0 60.644531-49.335938 109.980469-109.980469 109.980469zm0 0"/><path d="m399.34375 66.285156c-22.8125 0-41.367188 18.558594-41.367188 41.367188 0 22.8125 18.554688 41.371094 41.367188 41.371094s41.371094-18.558594 41.371094-41.371094-18.558594-41.367188-41.371094-41.367188zm0 52.71875c-6.257812 0-11.351562-5.09375-11.351562-11.351562 0-6.261719 5.09375-11.351563 11.351562-11.351563 6.261719 0 11.355469 5.089844 11.355469 11.351563 0 6.257812-5.09375 11.351562-11.355469 11.351562zm0 0"/></svg>
                      {# <svg viewBox="0 0 512 512">
                        <use xlink:href="/assets/images/sprite.svg#instagram-ico"></use>
                      </svg> #}
                    </a>
                  </li>
                </ul>
              </div>
              <!-- End company info -->
            </div>
            <div class="col-xl-2 col-lg-3 col-md-3 col-7 item custom-footer-item">
              <div class="footer-item">
                <h4 class="footer-item-title">{{ footer[locale].menu.title }}</h4>
                <nav class="footer-nav">
                  <ul class="footer-mnu">
                    {% for page in footer[locale].menu.links %}
                      {% if page.label == "Home" or page.label == "Inicio" %}
                      {% else %}
                        <li>
                          <a onclick="mixpanel.track('Go ' + page.label + ' Button')" href="{{ page.link }}" class="hover-link" data-title="{{ page.label }}">
                            <span>{{ page.label }}</span>
                          </a>
                        </li>
                      {% endif %}
                    {% endfor %}
                  </ul>
                </nav>
              </div>
            </div>
            <div class="col-xl-2 col-lg-3 col-md-3 col-7 item custom-footer-item">
              <div class="footer-item">
                <h4 class="footer-item-title">{{ footer[locale].offer.title }}</h4>
                <nav class="footer-nav">
                  <ul class="footer-mnu">
                    {% for page in footer[locale].offer.links %}
                      <li>
                        <li>
                          <a onclick="mixpanel.track('Go ' + page.label + ' Button')" href="{{ page.link }}" class="hover-link" data-title="{{ page.label }}">
                            <span>{{ page.label }}</span>
                          </a>
                        </li>
                      </li>
                    {% endfor %}
                  </ul>
                </nav>
              </div>
            </div>
            <div class="col-xl-2 col-lg-3 col-md-3 col-7 item custom-footer-item">
              <div class="footer-item">
                <h4 class="footer-item-title">{{ footer[locale].about.title }}</h4>
                <nav class="footer-nav">
                  <ul class="footer-mnu">
                    {% for page in footer[locale].about.links %}
                      <li>
                        <li>
                          <a onclick="mixpanel.track('Go ' + page.label + ' Button')" href="{{ page.link }}" class="hover-link" data-title="{{ page.label }}">
                            <span>{{ page.label }}</span>
                          </a>
                        </li>
                      </li>
                    {% endfor %}
                  </ul>
                </nav>
              </div>
            </div>
            <div class="col-xl-3 col-lg-4 col-md-4 col-8 item custom-footer-item">
              <div class="footer-item">
                <h4 class="footer-item-title">{{ footer[locale].contact.title }}</h4>
                <ul class="footer-contacts">
                  <li>
                    <i class="material-icons md-22">location_on</i>
                    <div class="footer-contact-info">{{ footer[locale].contact.links[0].item1  }}</div>
                  </li>
                  <li>
                    <i class="material-icons md-22 footer-contact-email">email</i>
                    <div class="footer-contact-info">
                      <a onclick="mixpanel.track('Go to Email Button')" href="mailto:mail@example.com">{{ footer[locale].contact.links[1].item1  }}</a>
                    </div>
                  </li>
                  <li>
                    <i class="material-icons md-22 ">phone</i>
                    <div class="footer-contact-info">
                      <a onclick="mixpanel.track('Go to Phone Button')" href="PhoneExample">{{ footer[locale].contact.links[2].item1  }}</a>
                      <br>
                      <a onclick="mixpanel.track('Go to Phone Button')" href="PhoneExample">{{ footer[locale].contact.links[2].item2  }}</a>
                    </div>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="footer-bottom">
        <div class="container">
          <div class="row justify-content-between items">
            <div class="col-md-auto col-12 item">
              <div class="copyright">{{ footer[locale].copyright.copyright }}</div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </footer>
{% endblock macro_custom_footer %}